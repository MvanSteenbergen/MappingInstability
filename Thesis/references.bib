@article{white_context-specific_2002,
	title = {Context-{Specific} {Tolerance} to the {Ataxic} {Effects} of {Alcohol}},
	volume = {72},
	issn = {0091-3057},
	doi = {10.1016/S0091-3057(01)00731-6},
	abstract = {Investigated whether context-specific tolerance can occur to the ataxic effects of alcohol in male rats. Baseline levels of motor coordination were assessed using a tilting plane apparatus (TILT). During a 7-day tolerance acquisition phase, subjects (Ss) were injected with 1.5 g/kg alcohol or saline in a novel testing room and were then placed in the TILT for 20 min. Approximately 5 h after the 1st injection, Ss received a 2nd injection in the colony room and were then placed in their home cages. The paired (P) group, received alcohol in the testing room and saline in the colony room; the unpaired (UP) group received saline in the testing room and alcohol in the colony room; controls (Cs) received saline in both environments. Ss were injected with alcohol after tolerance acquisition and tested for ataxia in the TILT. S in the P group were less ataxic than Cs during all 4 testing blocks following alcohol administration. UP Ss were less ataxic than Cs only during the 15-min testing block. Relative to baseline, P Ss exhibited deficits only during the 5- and 10-min testing blocks, while UP Ss and Cs exhibited deficits during all 4 testing blocks. Tolerance to alcohol's ataxic effects can become conditioned to contextual cues present at the time of alcohol administration. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
	number = {1-2},
	journal = {Pharmacology, Biochemistry and Behavior},
	author = {White, Aaron M. and Roberts, Darryn C. and Best, Phillip J.},
	month = may,
	year = {2002},
	keywords = {Animals, Ataxia, Conditioning, Conditioning (Psychology), Contextual Associations, Contextual Cues, Cues, Drug Tolerance, Ethanol, Male, Rats, Rats Long-Evans, ataxic effects of alcohol, conditioning, context-specific tolerance, contextual cues, rats, tolerance acquisition},
	pages = {107--110},
}

@article{white_context-specific_2002-1,
