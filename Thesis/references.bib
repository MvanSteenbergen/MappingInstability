	note = {Publisher: Elsevier Science},
	keywords = {Adult, Computer Science, Computer Simulation, Humans, Lexical Access, Mental Lexicon, Neural Networks, Psycholinguistics, Speech, Speech Perception, Young Adult, computer science, computer simulations, lexical processing, lexical retrieval, mental lexicon, network sciences},
	pages = {1--32},
}

@book{van_van_zomeren_self_2016,
