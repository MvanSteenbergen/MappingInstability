@article{reddy_encoding_1983,
	title = {Encoding {Specificity} in {Free} {Recall}},
	volume = {9},
	issn = {0278-7393},
	doi = {10.1037/0278-7393.9.1.167},
	abstract = {Recorded the overt verbalizations produced by 72 college students during a study of a list of words. For Ss in the elaboration condition, vocalizations included verbal associations and descriptions of visual images. Ss in the story condition verbalized while using the story mnemonic. At recall, Ss either (a) again verbalized; (b) were given, as recall cues, transcripts of their study vocalizations minus list words; or (c) were given, as recall cues, transcripts of some other S's vocalizations. In general, level of recall was directly related to the degree to which the contextual information produced during encoding was reinstated during recall, thus supporting the encoding-specificity hypothesis. The authors suggest that the organization of episodic memory depends on the reliability with which internal context present during learning can be retrieved. If the internal cues are organized, as they are when using a mnemonic device, then they can be used in a systematic manner both at encoding and at retrieval to optimize recall. (25 ref) (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
