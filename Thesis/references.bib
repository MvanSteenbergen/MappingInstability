@article{carnine_utilization_1984,
	title = {Utilization of {Contextual} {Information} in {Determining} the {Meaning} of {Unfamiliar} {Words}},
	volume = {19},
	issn = {0034-0553},
	doi = {10.2307/747362},
	abstract = {Conducted a descriptive study (Study 1), using 114 Ss from Grades 4–6, to examine the differential effects of form and proximity of contextual information on Ss' learning of unfamiliar words. Ss were given both a words-in-isolation and a words-in-context test. Study 2, using 37 same-age Ss, examined the differential effects of 3 intervention strategies designed to facilitate the use of contextual information in learning the meanings of unfamiliar words. Results of Study 1 suggest that Ss were better able to determine the meaning of unfamiliar words when (a) contextual clues were provided, (b) Ss were older, (c) the clues were in synonym rather than inference form, and (d) contextual clues were closer to the unfamiliar word. In Study 2, rule-plus-systematic-practice and systematic-practice-only conditions produced higher transfer scores than a no-intervention condition. Findings suggest that intermediate-grade students cannot be assumed to have adequate contextual analysis skills. (French \& Spanish abstracts) (50 ref) (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
