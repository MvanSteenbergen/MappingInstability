@article{kuntze_immersive_2001,
	title = {Immersive {Virtual} {Environments} in {Cue} {Exposure}},
	volume = {4},
	issn = {1094-9313},
	doi = {10.1089/109493101750527051},
	abstract = {Cue reactivity (CRT) to drug-related stimuli is a frequently observed phenomenon in drug addiction. CRT refers to a classical conditioned response pattern that occurs when an addict is exposed to drug-related stimuli. This response consists of physiological and cognitive reactions. Craving is believed to play an important role in the occurrence of relapse in the natural setting. Besides craving, other subjective cue-elicited reactions include withdrawal symptoms, drug-agonistic effects, and mood swings. Physiological reactions that have been investigated include skin conductance, heart rate, salivation, and body temperature. CRT is an important factor in addiction to alcohol, nicotine, opiates, and cocaine. Cue exposure treatment (CET) refers to a manualized, repeated exposure to drug-related cues, aimed at the reduction of CRT by extinction. In CET, different stimuli are presented (e.g., slides, video tapes, pictures, paraphernalia) in nonrealistic, experimental settings. Most often assessments consist in subjective ratings by craving scales. Using 25–45 yr old opioid dependent males, this pilot study attempted to show that immersive virtual reality is as good or even better in eliciting subjective and physiological craving symptoms as classical devices. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
	number = {4},
	urldate = {2018-11-26},
	journal = {CyberPsychology \& Behavior},
	author = {Kuntze, Marcus F. and Stoermer, Robert and Mager, Ralph and Roessler, Andreas and Mueller-Spahn, Franz and Bullinger, Alex H.},
	month = aug,
	year = {2001},
	keywords = {Adult, Arousal, Conditioned Responses, Conditioning Classical, Craving, Cues, Desensitization Psychologic, Drug Addiction, Drug Rehabilitation, Extinction Psychological, Heroin Dependence, Humans, Male, Middle Aged, Monitoring Physiologic, Motivation, Pilot Projects, Psychophysiology, Stimulation, Substance Withdrawal Syndrome, Therapy Computer-Assisted, User-Computer Interface, Virtual Reality, cue exposure treatment, cue reactivity to drug-related stimuli, drug addicts, immersive virtual reality},
