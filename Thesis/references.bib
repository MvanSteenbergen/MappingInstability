	keywords = {Clinical methodology, Clinical theory, Psychological networks, Psychometrics, Time series models},
	pages = {104011},
}

@article{briggs_interpreting_2019,
	title = {Interpreting and {Visualizing} the {Unit} of {Measurement} in the {Rasch} {Model}},
	volume = {146},
	issn = {0263-2241},
	doi = {10.1016/j.measurement.2019.07.035},
	abstract = {One of the more attractive features of the Rasch Model as a paradigm for measurement in the human sciences is that it has the potential to locate respondents on a test or survey instrument on a scale with equal intervals. Wright Maps (also known as person-item maps) are sometimes used as graphical visualizations of such scales, but they are frequently very difficult to interpret. One reason for this is that they are premised on an arbitrary unit of measurement, the logit. In this article an approach is illustrated whereby a new reference unit can be established for the interpretation and visualization of differences along the scale. This new unit is premised on the substantive choice of two locations on the scale that represent benchmark differences in the psychological attribute of interest. The invariance of the unit to differences in the context in which the measuring instrument was administered is then subject to empirical evaluation.},
	urldate = {2024-02-05},
