@article{bach_calibrating_2020,
	title = {Calibrating the experimental measurement of psychological attributes},
	volume = {4},
	copyright = {2020 Springer Nature Limited},
	issn = {2397-3374},
	url = {https://www.nature.com/articles/s41562-020-00976-8},
	doi = {10.1038/s41562-020-00976-8},
	abstract = {Behavioural researchers often seek to experimentally manipulate, measure and analyse latent psychological attributes, such as memory, confidence or attention. The best measurement strategy is often difficult to intuit. Classical psychometric theory, mostly focused on individual differences in stable attributes, offers little guidance. Hence, measurement methods in experimental research are often based on tradition and differ between communities. Here we propose a criterion, which we term ‘retrodictive validity’, that provides a relative numerical estimate of the accuracy of any given measurement approach. It is determined by performing calibration experiments to manipulate a latent attribute and assessing the correlation between intended and measured attribute values. Our approach facilitates optimising measurement strategies and quantifying uncertainty in the measurement. Thus, it allows power analyses to define minimally required sample sizes. Taken together, our approach provides a metrological perspective on measurement practice in experimental research that complements classical psychometrics.},
	language = {en},
	number = {12},
	urldate = {2024-04-20},
	journal = {Nature Human Behaviour},
	author = {Bach, Dominik R. and Melinščak, Filip and Fleming, Stephen M. and Voelkle, Manuel C.},
	month = dec,
	year = {2020},
	note = {Publisher: Nature Publishing Group},
	keywords = {Human behaviour, Sociology},
	pages = {1229--1235},
}

