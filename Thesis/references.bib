	note = {Publisher: Elsevier Science},
	keywords = {Child, Child Language, Childhood Development, Cognition, Dutch children, English as Second Language, English as a second language, Female, Foreign Language Learning, Friends, Humans, Male, Neuropsychological Tests, Recognition (Psychology), Verbal Learning, Vocabulary, Word Recognition, child cognition, word recognition},
