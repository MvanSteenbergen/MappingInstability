	keywords = {Dutch language, Foreign Language Learning, Language Proficiency, Linguistics, Measurement, Native Language, West European first languages, cognate linguistics, explanative power, genetic linguistic distance measures, second language acquisition, second language learners},
	pages = {401--432},
