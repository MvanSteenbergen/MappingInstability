@article{suzuki_flexible_2018,
	title = {The {Flexible} {Lexical} {Inferencing} {Processes} of {Japanese} {EFL} {Learners}: {A} {Think}-{Aloud} {Study}},
	volume = {29},
	shorttitle = {The {Flexible} {Lexical} {Inferencing} {Processes} of {Japanese} {EFL} {Learners}},
	doi = {10.20581/arele.29.0_145},
	abstract = {When inferring the meanings of unknown words, it is important to flexibly use available morphemes and contexts simultaneously because, while the meanings of transparent words can be inferred from their morphemes alone (e.g., sunlight), the meanings of opaque words cannot (e.g., honeymoon). The present study examined how English as a foreign language learners change their inferential processes depending on the semantic transparency (consistency) between morphological and contextual information. A total of 12 Japanese undergraduate and graduate students inferred the meanings of unknown words while reporting the contents of their thoughts. The target words were pseudo compounds whose literal meanings were either consistent or inconsistent with their contexts (transparent and opaque targets). Think-aloud protocols suggested that participants generally inferred meanings that fit the contexts. Also, depending on semantic consistency, the frequency with which participants used various strategies, from linguistic clues to metacognitive monitoring or evaluation of their own inferential processes, differed. In addition, a qualitative analysis of literal interpretations of opaque targets, albeit a small number of them, showed that these inferences were not always attributable to ignoring context. In fact, some inferential protocols reflected participants' mental efforts to make literal, morpheme-based interpretations meaningful within the context.},
	journal = {全国英語教育学会紀要},
	author = {Suzuki, Kentaro},
	year = {2018},
	pages = {145--160},
}

@article{suzuki_flexible_nodate,
	title = {The {Flexible} {Lexical} {Inferencing} {Processes} of {Japanese} {EFL} {Learners}: {A} {Think}-{Aloud} {Study}},
	abstract = {When inferring the meanings of unknown words, it is important to flexibly use available morphemes and contexts simultaneously because, while the meanings of transparent words can be inferred from their morphemes alone (e.g., sunlight), the meanings of opaque words cannot (e.g., honeymoon). The present study examined how English as a foreign language learners change their inferential processes depending on the semantic transparency (consistency) between morphological and contextual information. A total of 12 Japanese undergraduate and graduate students inferred the meanings of unknown words while reporting the contents of their thoughts. The target words were pseudo compounds whose literal meanings were either consistent or inconsistent with their contexts (transparent and opaque targets). Think-aloud protocols suggested that participants generally inferred meanings that fit the contexts. Also, depending on semantic consistency, the frequency with which participants used various strategies, from linguistic clues to metacognitive monitoring or evaluation of their own inferential processes, differed. In addition, a qualitative analysis of literal interpretations of opaque targets, albeit a small number of them, showed that these inferences were not always attributable to ignoring context. In fact, some inferential protocols reflected participants' mental efforts to make literal, morpheme-based interpretations meaningful within the context.},
	author = {Suzuki, Kentaro},
	pages = {16},
}

@article{suk_more_2021,
	title = {The {More} {You} {Know}, the {Less} {You} like: {A} {Comparative} {Study} of {How} {News} and {Political} {Conversation} {Shape} {Political} {Knowledge} and {Affective} {Polarization}},
	issn = {2057-0473},
	shorttitle = {The {More} {You} {Know}, the {Less} {You} {Like}},
	doi = {10.1177/20570473211063237},
	abstract = {The contemporary communication ecology contributes to affective polarization by presenting us with extreme exemplars of disliked groups. News exposure that is associated with political discussion networks is related to greater political knowledge, yet unlike previous eras where political knowledge and tolerance went hand in hand, this is no longer the case. We employ a comparative design to examine this idea among two democracies with differing levels of journalistic professionalism and political system: Mexico and the United States. Results show that greater political knowledge is associated with affective polarization, especially for the United States. Furthermore, there was a significant indirect path between media use and affective polarization, mediated through homogeneous political talk and political knowledge, but not in Mexico.},
	urldate = {2022-01-07},
	journal = {Communication and the Public},
	author = {Suk, Jiyoun and Coppini, David and Muñiz, Carlos and Rojas, Hernando},
	month = dec,
	year = {2021},
	note = {Publisher: SAGE Publications},
	keywords = {Affective polarization, comparative analysis, political conversation, political knowledge},
	pages = {20570473211063237},
}

@article{sturmberg_it_2017,
	title = {It {Is} {Complicated}! - {Misunderstanding} the {Complexities} of '{Complex}'},
	volume = {23},
	issn = {1365-2753},
	doi = {10.1111/jep.12579},
	abstract = {Terminology matters - as Lakoff emphasised, words and phrases evoke powerful images and frames of understanding. It is for that reason that we need to discern and use appropriately the term complex/complexity in the health science/professional/policy domain. Complex is the fashionable term used when in reality one means 'complicated', 'difficult to understand' or 'multiple simultaneous actions'. However, this is not what complex means. The Latin term means 'entwined/interwoven' - a structural characteristic describing systems. Complexity arises from the interactions between structurally connected entities - a functional characteristic of a system. The basis of scientific rigor is a clear understanding of a discipline's epistemology. Complexity refers to the emergence of outcomes from the interactions of a system's constituent components (and thus has nothing in common with the colloquial meaning of complicatedness).},
	number = {2},
	journal = {Journal of Evaluation in Clinical Practice},
	author = {Sturmberg, Joachim P. and Martin, Carmel M. and Katerndahl, David A.},
	month = apr,
	year = {2017},
	pmid = {27307382},
	keywords = {Delivery of Health Care, Humans, Systems Theory, Terminology as Topic, multimorbidity, patient-centred care, person-centred medicine},
	pages = {426--429},
}

