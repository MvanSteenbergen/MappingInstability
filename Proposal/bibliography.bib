@article{stichtReacquisitionHeroinCocaine2010,
  title = {Reacquisition of Heroin and Cocaine Place Preference Involves a Memory Consolidation Process Sensitive to Systemic and Intra-Ventral Tegmental Area Naloxone},
  author = {Sticht, Martin and Mitsubata, Jackie and Tucci, Mark and Leri, Francesco},
  year = {2010},
  month = feb,
  journal = {Neurobiology of Learning and Memory},
  volume = {93},
  number = {2},
  pages = {248--260},
  issn = {1074-7427},
  doi = {10.1016/j.nlm.2009.10.005},
  abstract = {To investigate the effect of naloxone on a putative memory consolidation process underlying reacquisition of heroin and cocaine conditioned place preference, four studies were conducted in male Sprague\textendash Dawley rats using a common procedure involving: place conditioning (0.3 or 1 mg/kg heroin or 20 mg/kg cocaine; \texttimes 4 sessions), extinction (vehicle \texttimes{} 4 sessions), and reconditioning (0 or 1 mg/kg heroin or 20 mg/kg cocaine; \texttimes 1 session). Systemic naloxone injections (0, 1 and 3 mg/kg) or bilateral intra-ventral tegmental area (VTA) naloxone methiodide infusions (2 nmol in 0.5 {$\mu$}l \texttimes{} side) were administered at different times following reconditioning. Post-reconditioning administration of naloxone dose-dependently blocked, attenuated and had no effect on reacquisition of heroin CPP when administered immediately, 1 h and 6 h after reconditioning, respectively. The highest dose of naloxone also blocked reacquisition of cocaine CPP, and did not produce a conditioned place aversion in heroin-na\"ive and heroin pre-treated animals. Post-reconditioning infusions in the VTA, but not in adjacent structures, blocked reacquisition of heroin CPP when administered immediately, but not 6 h, after reconditioning. These data suggest that reacquisition of drug-cues associations involves a memory consolidation process sensitive to manipulations of the endogenous opioid system, and indicate that opioid receptors in the VTA may be critically involved in the re-emergence of drug seeking behavior. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  keywords = {Animals,Central Nervous System Agents,cocaine,Cocaine,conditioned place preference,{Conditioning, Classical},Dopamine Uptake Inhibitors,{Dose-Response Relationship, Drug},heroin,Heroin,Male,Memory,memory consolidation,naloxone,Naloxone,Narcotic Antagonists,Narcotics,Neuropsychological Tests,Place Conditioning,rat,Rats,{Rats, Sprague-Dawley},reacquisition,Space Perception,Tegmentum,Time Factors,ventral tegmental area,Ventral Tegmental Area}
}

@article{stinsonEcologicalMomentaryAssessment2022,
