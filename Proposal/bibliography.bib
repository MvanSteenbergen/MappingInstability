@article{masShortTermWe2013,
  title = {In the {{Short Term We Divide}}, in the {{Long Term We Unite}}: {{Demographic Crisscrossing}} and the {{Effects}} of {{Faultlines}} on {{Subgroup Polarization}}},
  shorttitle = {In the {{Short Term We Divide}}, in the {{Long Term We Unite}}},
  author = {M{\"a}s, Michael and Flache, Andreas and Tak{\'a}cs, K{\'a}roly and Jehn, Karen A.},
  year = {2013},
  month = jun,
  journal = {Organization Science},
