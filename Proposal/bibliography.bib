@article{wulfgrammDividedMarketDivided2017,
  title = {Divided by the {{Market}}, {{Divided}} by the {{State}}: {{Distribution}}, {{Redistribution}} and {{Welfare Attitudes}} in 47 {{Countries}}},
  shorttitle = {Divided by the {{Market}}, {{Divided}} by the {{State}}},
  author = {Wulfgramm, Melike and Starke, Peter},
