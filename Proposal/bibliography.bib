@article{birakConditionedToleranceEffects2011,
  title = {Conditioned Tolerance to the Effects of Alcohol on Inhibitory Control in Humans},
  author = {Birak, Kulbir Singh and Higgs, Suzanne and Terry, Philip},
  year = {2011},
