@article{papenbergSmallBeautiesLarge2017,
  title = {Of Small Beauties and Large Beasts: {{The}} Quality of Distractors on Multiple-Choice Tests Is More Important than Their Quantity},
  shorttitle = {Of Small Beauties and Large Beasts},
