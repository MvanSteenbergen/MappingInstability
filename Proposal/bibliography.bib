@techreport{vanderveldeAlleviatingColdStart2020,
  type = {Preprint},
  title = {Alleviating the {{Cold Start Problem}} in {{Adaptive Learning}} Using {{Data-Driven Difficulty Estimates}}},
  author = {{van der Velde}, Maarten and Sense, Florian and {borst}, jelmer and {van Rijn}, Hedderik},
  year = {2020},
  month = jun,
  institution = {{PsyArXiv}},
  doi = {10.31234/osf.io/hf2vw},
  urldate = {2020-08-17},
  abstract = {An adaptive learning system offers a digital learning environment that adjusts itself to the individual learner. By refining its internal model of the learner over time, such a system continually improves its ability to present appropriate exercises that maximise learning gains. In many cases, there is an initial mismatch between the internal model and the learner's actual ability, causing a `cold start' during which the system is poorly adjusted to the learner. In this study, we implemented several strategies for mitigating this cold start problem in an adaptive fact learning system and experimentally tested their effect on learning performance. The strategies included predicting difficulty for individual learner-fact pairs, individual learners, individual facts, and the set of facts as a whole.  We found that cold start mitigation improved learning outcomes, provided that there was sufficient variability in the difficulty of the study material. Informed individualised predictions allowed the system to schedule learners' study time more effectively, leading to an increase in response accuracy during the learning session as well as improved retention of the studied items afterwards. Our findings show that addressing the cold start problem in adaptive learning systems can have a real impact on learning outcomes. We expect this to be particularly valuable when applied to the diverse materials and learners found in real-world educational settings.},
  file = {C:\Users\lenovo\Zotero\storage\KW9CDCFP\van der Velde et al. - 2020 - Alleviating the Cold Start Problem in Adaptive Lea.pdf}
}

