@article{makiEconomicEpistemologyHopes2005,
  title = {Economic {{Epistemology}}: {{Hopes}} and {{Horrors}}},
  shorttitle = {Economic {{Epistemology}}},
  author = {M{\"a}ki, Uskali},
  year = {2005},
  month = feb,
  journal = {Episteme},
  volume = {1},
