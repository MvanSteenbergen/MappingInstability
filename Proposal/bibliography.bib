@article{oteroBiodiversityPolicyEconomic2020,
  title = {Biodiversity Policy beyond Economic Growth},
  author = {Otero, Iago and Farrell, Katharine N. and Pueyo, Salvador and Kallis, Giorgos and Kehoe, Laura and Haberl, Helmut and Plutzar, Christoph and Hobson, Peter and {Garc{\'i}a-M{\'a}rquez}, Jaime and {Rodr{\'i}guez-Labajos}, Beatriz and Martin, Jean-Louis and Erb, Karl-Heinz and Schindler, Stefan and Nielsen, Jonas and Skorin, Teuta and Settele, Josef and Essl, Franz and {G{\'o}mez-Baggethun}, Erik and Brotons, Llu{\'i}s and Rabitsch, Wolfgang and Schneider, Fran{\c c}ois and Pe'er, Guy},
  year = {2020},
  journal = {Conservation Letters},
